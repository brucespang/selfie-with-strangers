<% include ../../header %>

<h1>THE ADMIN ZONE | Manage Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Username</th>
    <th>Email</th>
    <th>Admin?</th>
  </tr>

	<% users.forEach(function(user) { %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.login %></td>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
    </tr>
  <% }) %>
</table>

<% include ../../footer %>
